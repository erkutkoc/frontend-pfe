{
    "name": "frontend-market-vinci",
    "version": "0.0.1",
    "engines": {
        "node": "16.x.x",
        "npm": "8.1"
    },
    "scripts": {
        "start": "node public/index.js",
        "dev": "concurrently \"yarn run dev:only -p 5000\" \"yarn run tailwind:watch\"",
        "build": "svelte-kit build",
        "package": "svelte-kit package",
        "preview": "svelte-kit preview",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "dev:only": "svelte-kit dev",
        "build:only": "svelte-kit build",
        "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
        "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^3.0.0",
        "@sveltejs/adapter-node": "next",
        "@sveltejs/kit": "next",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "autoprefixer": "^10.4.0",
        "concurrently": "^6.4.0",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^3.2.1",
        "postcss-cli": "^9.0.2",
        "postcss-load-config": "^3.1.0",
        "prettier": "^2.4.1",
        "prettier-plugin-svelte": "^2.4.0",
        "svelte": "^3.44.2",
        "svelte-loading-spinners": "^0.1.7",
        "svelte-materialify": "^0.3.11",
        "svelte-preprocess": "^4.10.0",
        "tailwindcss": "^2.2.19"
    },
    "type": "module",
    "dependencies": {
        "@splidejs/svelte-splide": "^0.1.15",
        "@tailwindcss/forms": "^0.3.4",
        "axios": "^0.24.0",
        "bootstrap": "^5.1.3",
        "bulma": "^0.9.3",
        "node-sass": "^7.0.0",
        "popper.js": "^1.14.3",
        "pusher-js": "^7.0.3",
        "sapper-environment": "^1.0.1",
        "svelte-particles": "^1.20.5",
        "svelte-tabs": "^1.1.0",
        "typescript": "^4.5.3",
        "vite": "^2.7.1",
        "yarn": "^1.22.17"
    }
}
